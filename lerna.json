{
  "lerna": "3.13.1",
  "packages": [
    "packages/*"
  ],
  "version": "0.1.0-beta.1",
  "npmClient": "yarn",
  "npmClientArgs": [
    "--no-lockfile"
  ],
  "changelog": {
    "repo": "shahzaibkhalid/shaizei",
    "labels": {
      "new feature": ":rocket: New Feature",
      "breaking change": ":boom: Breaking Change",
      "bug fix": ":bug: Bug Fix",
      "enhancement": ":nail_care: Enhancement",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal",
      "underlying tools": ":hammer: Underlying Tools"
    },
    "cacheDir": ".changelog"
  },
  "command": {
    "publish": {
      "message": "chore(release): publish %s",
      "allowBranch": "master"
    }
  }
}
